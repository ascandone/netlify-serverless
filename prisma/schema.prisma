generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todos {
  id         BigInt   @id @unique @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  text       String   @default("")
  completed  Boolean  @default(false)
}
